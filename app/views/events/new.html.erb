<div class="container">
    <h1>Event#new</h1>
  <p>Find me in app/views/event/new.html.erb</p>

  <%= simple_form_for(@event) do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :venue_name %>
    <%= f.input :venue_address %>
    <%= f.input :est_guests %>
    <%= f.input :total_budget %>
    <%= f.button :button, "Create Event" %>
  <% end %>
</div>
